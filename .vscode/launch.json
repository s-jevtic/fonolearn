  "version": "0.2.0",
  "configurations": [
    {
      "name": "ng serve",
      "type": "firefox",
      "request": "launch",
      "preLaunchTask": "npm: start",
      "url": "http://localhost:4200/",
      "webRoot": "${workspaceFolder}/src/app"
    },
    {
      "name": "ng test",
      "type": "firefox",
      "request": "launch",
      "preLaunchTask": "npm: test",
      "port": 9876,
      "url": "http://localhost:9876/debug.html",
      "webRoot": "${workspaceFolder}/src/app"
    },
    {
      "name": "ng serve (Chrome)",
      "type": "chrome",
      "request": "launch",
      "preLaunchTask": "npm: start",
      "url": "http://localhost:61234/",
      "webRoot": "${workspaceFolder}/src/app"
    },
    {
      "name": "ng test (Chrome)",
      "type": "chrome",
      "request": "launch",
      "preLaunchTask": "npm: test",
      "url": "http://localhost:61234/debug.html",
      "webRoot": "${workspaceFolder}/src/app"
    }
  ]
}
